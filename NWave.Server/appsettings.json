{
  "Logging": {

    "LogLevel": {

      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft": "Information"
      }
    },
    "Console": {
      "IncludeScopes": true,
      "LogLevel": {
        "Default": "Debug"
      }
    },
    "EventSource": {
      "LogLevel": {
        "Microsoft": "Debug"
      }
    },
    "TraceSource": {
      "LogLevel": {
        "Default": "Trace"
      }
    }
  },
  "AllowedHosts": "*"
}
